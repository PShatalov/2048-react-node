    "build-js": "npm run set-env -- tsc --project 'tsconfig.build.json'",
    "build": "npm-run-all build-js",
    "format": "tslint --project tsconfig.json -c tslint.json --fix 'src/**/*.ts'",
    "postinstall": "cd ../frontend && npm install"
